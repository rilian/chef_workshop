{
  "users":["rails"],
  "additional_packages":[ "htop" ],
  "nginx":{
    "version":"1.3.6",
    "default_site_enabled":false
  },
  "postgresql":{
    "setup_items":["rails"],
    "version":"9.1"
  },
  "run_list":[
    "recipe[additional_packages]",
    "recipe[nginx::source]",
    "recipe[user::data_bag]",
    "recipe[postgresql::apt_postgresql_ppa]",
    "recipe[postgresql::server]",
    "recipe[postgresql::setup]"
  ]
}